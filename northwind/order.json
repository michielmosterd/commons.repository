{
    "class": "northwindModel.order",
    "inherits": "commons.item",
    "type": "BASIC",
    
    "valueTypes": [
        {
            "name": "orderID",
            "dataType": "INTEGER",
            "required": true
        },
        {
            "name": "orderDate",
            "dataType": "DATE"
        },
        {
            "name": "requiredDate",
            "dataType": "DATE"
        },
        {
            "name": "shippedDate",
            "dataType": "DATE"
        },
        {
            "name": "freight",
            "dataType": "DECIMAL"
        },
        {
            "name": "shipName",
            "dataType": "TEXT"
        },
        {
            "name": "shipAddress",
            "dataType": "TEXT"
        },
        {
            "name": "shipCity",
            "dataType": "TEXT"
        },
        {
            "name": "shipRegion",
            "dataType": "TEXT"
        },
        {
            "name": "shipPostalCode",
            "dataType": "TEXT"
        },
        {
            "name": "shipCountry",
            "dataType": "TEXT"
        }
    ],
    "formulas": [
        { 
            "valueType": "name",
            "template": "Order #{{orderID}}"
        }
    ],
    "relations": [
        {
            "type": "northwindModel.customer",
            "required": false,
            "multiple": false
        },
        {
            "type": "northwindModel.employee",
            "required": false,
            "multiple": false
        },
        {
            "type": "northwindModel.shipper",
            "required": false,
            "multiple": false
        }
    ],
    "views": [
      {
        "defaultView": true,
        "relationView": true,
        "defaultSort": "orderID",
        "values": [
            { "type": "RELATION", "relatedClass": "northwindModel.customer" },
            { "name": "name" },
            { "name": "orderDate" }
        ]
      }
    ]
}