{
    "name": "product",
    "namespace": "northwindModel",
    "extendsType": "commons.item",
    "valueTypes": [
        {
            "name": "productID",
            "dataType": "INTEGER",
            "required": true
        },
        {
            "name": "productName",
            "dataType": "TEXT",
            "required": true
        },
        {
            "name": "quantityPerUnit",
            "dataType": "TEXT"
        },
        {
            "name": "unitPrice",
            "dataType": "DECIMAL"
        },
        {
            "name": "unitsInStock",
            "dataType": "INTEGER"
        },
        {
            "name": "unitsOnOrder",
            "dataType": "INTEGER"
        },
        {
            "name": "reorderLevel",
            "dataType": "INTEGER"
        },
        {
            "name": "discontinued",
            "dataType": "BOOLEAN",
            "required": true
        },
        {
            "name": "picture",
            "dataType": "IMAGE",
            "required": true
        }
    ],
    "formulas": [
        { 
            "valueType": "name",
            "formula": "{{productName}}"
        }
    ],
    "relations": [
        {
            "type": "northwindModel.category",
            "required": false,
            "multiple": false
        },
        {
            "type": "northwindModel.supplier",
            "required": false,
            "multiple": false
        }
    ],
    "views": [
      {
        "name": "Overview",
        "defaultView": true,
        "defaultSort": "name",
        "values": [
            "name",
            "northwindModel.category",
            "northwindModel.supplier"
        ]
      }
    ],
    "type": "BASIC"
}