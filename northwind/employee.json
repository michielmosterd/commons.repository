            "dataType": "TEXT",
            "required": true
        },
        {
            "name": "firstName",
            "dataType": "TEXT",
            "required": true
        },
        {
            "name": "title",
            "dataType": "TEXT"
        },
        {
            "name": "titleOfCourtesy",
            "dataType": "TEXT"
        },
        {
            "name": "birthDate",
            "dataType": "DATE"
        },
        {
            "name": "hireDate",
            "dataType": "DATE"
        },
        {
            "name": "address",
            "dataType": "TEXT"
        },
        {
            "name": "city",
            "dataType": "TEXT"
        },
        {
            "name": "region",
            "dataType": "TEXT"
        },
        {
            "name": "postalCode",
            "dataType": "TEXT"
        },
        {
            "name": "country",
            "dataType": "TEXT"
        },
        {
            "name": "homePhone",
            "dataType": "TEXT"
        },
        {
            "name": "extension",
            "dataType": "TEXT"
        },
        {
            "name": "notes",
            "dataType": "TEXT"
        },
        {
            "name": "photoPath",
            "dataType": "TEXT"
        }
